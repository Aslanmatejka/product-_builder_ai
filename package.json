{
  "name": "product-builder",
  "version": "1.0.0",
  "description": "AI-powered CAD and PCB manufacturing file generator",
  "main": "server/index.js",
  "scripts": {
    "start": "node server/index.js",
    "dev": "nodemon --config nodemon.json server/index.js",
    "client": "cd client && npm start",
    "dev:full": "concurrently \"npm run dev\" \"npm run client\"",
    "health-check": "node health-check.js",
    "install:all": "npm install && cd client && npm install",
    "build:client": "cd client && npm run build",
    "prepare:mvp": "node prepare-mvp.js",
    "verify:mvp": "node verify-mvp.js"
  },
  "keywords": [
    "CAD",
    "PCB",
    "AI",
    "manufacturing",
    "FreeCAD",
    "KiCad"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.20.0",
    "@supabase/supabase-js": "^2.94.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.1"
  }
}
